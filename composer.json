{
    "name": "acmephp/core",
    "description": "Raw implementation of the ACME protocol in PHP. Core of the other acmephp repositories.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/acmephp/core",
    "keywords": [ "ACME", "letsencrypt", "https", "encryption" ],
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com",
            "homepage": "http://titouangalopin.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-openssl": "*",
        "psr/log": "^1.0",
        "webmozart/assert": "^1.0",
        "guzzlehttp/guzzle": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.22",
        "sllh/php-cs-fixer-styleci-bridge": "^1.5.0"
    },
    "autoload": {
        "psr-4": {
            "AcmePhp\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcmePhp\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
